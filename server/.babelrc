{
  "presets": [
    "node5",
    "es2015",
    "stage-0",
    "react",
  ],
  "plugins": [
    "transform-class-properties",
    "add-module-exports",
    "transform-es2015-modules-commonjs",
    ["babel-import-from-root", {
      "rootPathSuffix": "server"
    }],
    "transform-decorators-legacy"
  ]
}
