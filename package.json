    "eslint-loader": "^0.12.0",
    "eslint-plugin-react": "^2.7.1",
    "express": "^4.13.3",
    "express-flash": "0.0.2",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "font-loader": "^0.1.2",
    "helmet": "^0.15.0",
    "history": "^1.13.1",
    "http-proxy": "^1.11.1",
    "immutable": "^3.7.4",
    "imports-loader": "^0.6.5",
    "invariant": "^2.1.2",
    "ioredis": "^1.14.0",
    "isomorphic-fetch": "^2.2.0",
